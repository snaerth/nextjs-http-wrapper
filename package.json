{
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "nextjs-http-method-wrapper",
    "nextjs-http-wrapper",
    "http-method-wrapper"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snaerth/nextjs-http-wrapper"
  },
  "name": "nextjs-http-wrapper",
  "author": "Snær Seljan Þóroddsson",
  "module": "dist/nextjs-http-wrapper.esm.js",
  "size-limit": [
    {
      "path": "dist/nextjs-http-wrapper.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/nextjs-http-wrapper.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "http-method-enum": "^1.0.0",
    "husky": "^8.0.1",
    "next": "^12.1.6",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
